<?xml version='1.0' encoding='utf-8'?>
<widget id="com.abmigroup.nnrx" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <preference name="Fullscreen" value="true" />
    <feature name="Device">
        <param name="android-package" value="org.apache.cordova.device.Device" />
    </feature>
    <feature name="DatePickerPlugin">
        <param name="android-package" value="com.plugin.datepicker.DatePickerPlugin" />
    </feature>
    <feature name="LocalNotification">
        <param name="android-package" value="de.appplant.cordova.plugin.localnotification.LocalNotification" />
    </feature>
    <feature name="Notification">
        <param name="android-package" value="org.apache.cordova.dialogs.Notification" />
    </feature>
    <feature name="SplashScreen">
        <param name="android-package" value="org.apache.cordova.splashscreen.SplashScreen" />
    </feature>
    <icon density="ldpi" src="/www/res/icons/android/icon-36-ldpi.png" />
    <icon density="mdpi" src="/www/res/icons/android/icon-48-mdpi.png" />
    <icon density="hdpi" src="/www/res/icons/android/icon-72-hdpi.png" />
    <icon density="xhdpi" src="/www/res/icons/android/icon-96-xhdpi.png" />
    <name>NeuroNEXT Rx</name>
    <description>
        Provides medication reminders for participants in the NeuroNEXT set of
        clinical trials.
    </description>
    <author email="mark@abmigroup.com" href="abmigroup.com">
        Advanced Biomedical Imaging Group
    </author>
    <content src="index.html" />
    <access origin="*" />
    <preference name="SplashScreen" value="ic_launcher" />
    <preference name="SplashScreenDelay" value="1000" />
    <splash src="www/res/screens/" />
</widget>
