<?xml version='1.0' encoding='utf-8'?>
<widget id="com.abmigroup.nnrx" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="AllowInlineMediaPlayback" value="false" />
    <preference name="AutoHideSplashScreen" value="true" />
    <preference name="BackupWebStorage" value="cloud" />
    <preference name="DisallowOverscroll" value="false" />
    <preference name="EnableViewportScale" value="false" />
    <preference name="FadeSplashScreen" value="true" />
    <preference name="FadeSplashScreenDuration" value=".25" />
    <preference name="KeyboardDisplayRequiresUserAction" value="true" />
    <preference name="MediaPlaybackRequiresUserAction" value="false" />
    <preference name="ShowSplashScreenSpinner" value="true" />
    <preference name="SuppressesIncrementalRendering" value="false" />
    <preference name="TopActivityIndicator" value="gray" />
    <preference name="GapBetweenPages" value="0" />
    <preference name="PageLength" value="0" />
    <preference name="PaginationBreakingMode" value="page" />
    <preference name="PaginationMode" value="unpaginated" />
    <feature name="LocalStorage">
        <param name="ios-package" value="CDVLocalStorage" />
    </feature>
    <feature name="Notification">
        <param name="ios-package" value="CDVNotification" />
    </feature>
    <feature name="Device">
        <param name="ios-package" value="CDVDevice" />
    </feature>
    <feature name="DatePicker">
        <param name="ios-package" value="DatePicker" />
    </feature>
    <preference name="Fullscreen" value="true" />
    <feature name="HiddenStatusbarOverlay">
        <param name="ios-package" value="APPHiddenStatusbarOverlay" />
    </feature>
    <feature name="LocalNotification">
        <param name="ios-package" onload="true" value="APPLocalNotification" />
        <param name="onload" value="true" />
    </feature>
    <feature name="SplashScreen">
        <param name="ios-package" value="CDVSplashScreen" />
        <param name="onload" value="true" />
    </feature>
    <icon height="60" src="www/res/icons/ios/icon-60.png" width="60" />
    <icon height="120" src="www/res/icons/ios/icon-60-2x.png" width="120" />
    <icon height="76" src="www/res/icons/ios/icon-76.png" width="76" />
    <icon height="152" src="www/res/icons/ios/icon-76-2x.png" width="152" />
    <icon height="40" src="www/res/icons/ios/icon-40.png" width="40" />
    <icon height="80" src="www/res/icons/ios/icon-40-2x.png" width="80" />
    <icon height="57" src="www/res/icons/ios/icon-57.png" width="57" />
    <icon height="114" src="www/res/icons/ios/icon-57-2x.png" width="114" />
    <icon height="72" src="www/res/icons/ios/icon-72.png" width="72" />
    <icon height="144" src="www/res/icons/ios/icon-72-2x.png" width="144" />
    <icon height="29" src="www/res/icons/ios/icon-29.png" width="29" />
    <icon height="58" src="www/res/icons/ios/icon-29-2x.png" width="58" />
    <icon height="50" src="www/res/icons/ios/icon-50.png" width="50" />
    <icon height="100" src="www/res/icons/ios/icon-50-2x.png" width="100" />
    <name>NeuroNEXT Rx</name>
    <description>
        Provides medication reminders for participants in the NeuroNEXT set of
        clinical trials.
    </description>
    <author email="mark@abmigroup.com" href="abmigroup.com">
        Advanced Biomedical Imaging Group
    </author>
    <content src="index.html" />
    <access origin="*" />
    <preference name="SplashScreen" value="ic_launcher" />
    <preference name="SplashScreenDelay" value="1000" />
    <splash src="www/res/screens/" />
    <feature name="Console">
        <param name="ios-package" value="CDVLogger" />
    </feature>
</widget>
